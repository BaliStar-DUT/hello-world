<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="hibernate.ShadowSocksLog" table="shadow_log">
		<id name="id" type="int" column="id">
            <generator class="native"/>
		</id>
    <property name="datetime" type="date" not-null="true" column="datetime"/>
    <property name="type" type="string" not-null="true" column="type"/>
    <property name="request" type="string" not-null="true" column="request"/>
    <property name="fromAd" type="string" not-null="true" column="fromAd"/>

	</class>

</hibernate-mapping>
